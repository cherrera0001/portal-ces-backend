{
  "scripts": {
    "test": "NODE_PATH=./src nyc --check-coverage --lines 50 --branches 10 --statements 40 --functions 20 --reporter=lcov --reporter=text-summary node ./node_modules/.bin/_mocha --timeout 12000 --exit ./test/**/**/*.test.js",
    "start": "NODE_PATH=./src DEBUG='app, route, mongo' node src/index.js",
    "dev": "npm run fixtures && NODE_PATH=./src DEBUG='app, route, mongo' nodemon src/index.js",
    "fixtures": "NODE_PATH=./src node src/fixtures/index.js",
    "prestart": "npm run fixtures"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && docker-compose run api npm test"
    }
  },
